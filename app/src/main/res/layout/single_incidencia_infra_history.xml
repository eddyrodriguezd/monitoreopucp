<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/singleInfraIncidencia_ConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textViewSingleInfraIncidenciaTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="@string/Title"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_TitleValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/Incidencia_TitleValue"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSingleInfraIncidenciaTitle"
        app:layout_constraintStart_toEndOf="@+id/textViewSingleInfraIncidenciaTitle"
        app:layout_constraintTop_toTopOf="@+id/textViewSingleInfraIncidenciaTitle" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_AnotacionesValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/Incidencia_AnnotationValue"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSingleInfraIncidencia_Anotaciones"
        app:layout_constraintStart_toEndOf="@+id/textViewSingleInfraIncidencia_Anotaciones"
        app:layout_constraintTop_toTopOf="@+id/textViewSingleInfraIncidencia_Anotaciones"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidenciaStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/Status"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textViewSingleInfraIncidenciaRegisterDate"
        app:layout_constraintTop_toBottomOf="@+id/textViewSingleInfraIncidenciaRegisterDate" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_CheckDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/CheckDate"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textViewSingleInfraIncidenciaStatus"
        app:layout_constraintTop_toBottomOf="@+id/textViewSingleInfraIncidenciaStatus" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_RegisterDateValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/Incidencia_DateValue"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSingleInfraIncidenciaRegisterDate"
        app:layout_constraintStart_toEndOf="@+id/textViewSingleInfraIncidenciaRegisterDate"
        app:layout_constraintTop_toTopOf="@+id/textViewSingleInfraIncidenciaRegisterDate"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidenciaRegisterDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/RegisterDate"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textViewSingleInfraIncidenciaStatus2"
        app:layout_constraintTop_toBottomOf="@+id/textViewSingleInfraIncidenciaStatus2" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_Anotaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/Annotations"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textViewSingleInfraIncidencia_CheckDate"
        app:layout_constraintTop_toBottomOf="@+id/textViewSingleInfraIncidencia_CheckDate" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_StatusValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/Incidencia_StatusValue"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSingleInfraIncidenciaStatus"
        app:layout_constraintStart_toEndOf="@+id/textViewSingleInfraIncidenciaStatus"
        app:layout_constraintTop_toTopOf="@+id/textViewSingleInfraIncidenciaStatus"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_CheckDateValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/Incidencia_DateValue"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSingleInfraIncidencia_CheckDate"
        app:layout_constraintStart_toEndOf="@+id/textViewSingleInfraIncidencia_CheckDate"
        app:layout_constraintTop_toTopOf="@+id/textViewSingleInfraIncidencia_CheckDate"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidencia_UserValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/Incidencia_StudentValue"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSingleInfraIncidenciaStatus2"
        app:layout_constraintStart_toEndOf="@+id/textViewSingleInfraIncidenciaStatus2"
        app:layout_constraintTop_toTopOf="@+id/textViewSingleInfraIncidenciaStatus2" />

    <TextView
        android:id="@+id/textViewSingleInfraIncidenciaStatus2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/RegisteredBy"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textViewSingleInfraIncidenciaTitle"
        app:layout_constraintTop_toBottomOf="@+id/textViewSingleInfraIncidenciaTitle" />

    <ImageView
        android:id="@+id/imageViewInfraIncidencia"
        android:layout_width="0dp"
        android:layout_height="140dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="16dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewSingleInfraIncidencia_Anotaciones"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>